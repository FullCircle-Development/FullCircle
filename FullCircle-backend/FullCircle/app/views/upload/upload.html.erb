<html>
<%= stylesheet_link_tag "upload", :media => "all" %>
<%= stylesheet_link_tag "application", :media => "all" %>
<head>
  <div>
    <%= image_tag "fullcircle.png", :class => "logo" %>
  </div>
  <nav>
    <li class="dropdown">
      <a href="javascript:void(0)" class="dropbtn">Team Building</a>
      <div class="dropdown-content">
        <a href="awards">Awards</a>
        <a href="fundraising">Fundraising</a>
        <a href="outreach">Outreach</a>
      </div>
    </li>
    <li class="dropdown">
    <a href="javascript:void(0)" class="dropbtn">Resources</a>
    <div class="dropdown-content">
      <a href="mechanical">Mechanical</a>
      <a href="programming">Programming</a>
      <a href="electrical">Electrical</a>
    </div>
    <li><a href="about_us">About Us</a></li>
    <li><a href="upload">Upload A Resource</a></li>
  </nav>
</head>
<body>
<%= form_tag '/process', multipart: true do %>
  <p></p>
  <%= file_field 'Resource', :file ,class: "formbutton" %>
  <p>Type of Resource: </p>
  <%= select_tag :type_of_resource ,options_for_select([['Awards'], ['Fundraising'], ['Outreach'], ['Mechanical'], ['Programming'], ['Electrical']]) %>
  <p></p>
  Descripton:
  <p></p>
  <%= text_area_tag :description %>
  <p></p>
  <%= submit_tag "Submit", class: "formbutton" %>
<% end %>
</body>
</html>
